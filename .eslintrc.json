{
  "extends": [
    "airbnb-base",
    "prettier",
    "prettier/react"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "globals": {
    "React": true
  },
  "rules": {
    "import/prefer-default-export": "off"
  }
}